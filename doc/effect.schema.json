{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "The tracks of the effect",
  "type": "object",
  "properties": {
    "name": {
      "description": "Name of the effect.",
      "type": "string"
    },
    "uuid": {
      "description": "UUID of the effect; Same as name.",
      "type": "string"
    },
    "keep": {
      "description": "Not in use.",
      "type": "string",
      "enum": [ "True", "False" ]
    },
    "priority": {
      "description": "Not in use.",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "tracks": {
      "description": "The list of tracks in the effect.",
      "type": "array",
      "items": {
        "description": "A track of the effect.",
        "type": "object",
        "properties": {
          "start_time": {
            "description": "Start time of the track in milliseconds.",
            "type": "integer",
            "minimum": 0,
            "maximum": 65535
          },
          "end_time": {
            "description": "End time of the track in milliseconds.",
            "type": "integer",
            "minimum": 0,
            "maximum": 65535
          },
          "stop_name": {
            "description": "Not in use.",
            "type": "string"
          },
          "start_intensity": {
            "description": "Starting intensity of the track.",
            "type": "integer",
            "minimum": 0,
            "maximum": 150
          },
          "end_intensity": {
            "description": "Ending intensity of the track.",
            "type": "integer",
            "minimum": 0,
            "maximum": 150
          },
          "intensity_mode": {
            "type": "string",
            "description": "Intensity mode of the track.",
            "enum": [ "Const", "Fade", "FadeInAndOut" ]
          },
          "action_type": {
            "type": "string",
            "description": "Action type of the track.",
            "enum": [ "Shake", "Electrical" ]
          },
          "once": {
            "type": "string",
            "description": "Whether the object performs an action only once. Effective when action_type is 'Electrical'.",
            "enum": [ "True", "False" ]
          },
          "interval": {
            "description": "Interval of the track in milliseconds. Effective when action_type is 'Electrical'.",
            "type": "integer",
            "minimum": 0,
            "maximum": 255
          },
          "index": {
            "description": "The list of actuators to apply the track to.",
            "type": "array",
            "items": {
              "description": "Index of the actuator",
              "type": "integer",
              "minimum": 0,
              "maximum": 255
            }
          }
        }
      }
    }
  }
}